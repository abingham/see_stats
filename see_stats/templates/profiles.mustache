<!DOCTYPE html>
<html>
<head>
    <title>Profile listing</title>
    {{> common_css}}
</head>
<body>
{{> header}}
<div class="container-fluid">
  {{#entries}}
  <div class="row-fluid">
    <div class="span6">
      <a href="/profile/{{_id}}">{{description}}</a>
    </div>
    <div class="span2">
      {{userid}}
    </div>
    <div class="span2">
      {{timestamp}}
    </div>
  </div>
  {{/entries}}
</div>
</body>
{{> common_js}}
</html>
