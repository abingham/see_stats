<!DOCTYPE html>
<html>
<head>
    <title>Profile listing</title>
    {{> common_css}}
</head>
<body>
{{> header}}
<div class="container-fluid">
  <div class="row-fluid">
    <table class="table table-striped table-bordered">
      <thead>
        <tr>
          <th>Description</th>
          <th>User</th>
          <th>Timestamp</th>
        </tr>
      </thead>
      <tbody>
        {{#entries}}
        <tr>
          <td><a href="/profile/{{_id}}">{{description}}</a></td>
          <td>{{userid}}</td>
          <td>{{timestamp}}</td>
        </tr>
        {{/entries}}
      </tbody>
    </table>
  </div>
</div>
</body>
{{> common_js}}
</html>
